{
  "name": "@lynx-js/luna-tokens",
  "version": "0.1.1",
  "description": "Design tokens for the L.U.N.A project.",
  "keywords": [
    "luna",
    "tokens",
    "design tokens",
    "theme",
    "colors"
  ],
  "license": "Apache-2.0",
