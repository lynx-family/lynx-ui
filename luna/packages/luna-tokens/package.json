      "require": "./dist/lunaris-light.cjs"
    },
    "./lunaris-dark": {
      "types": "./dist/lunaris-dark.d.ts",
      "import": "./dist/lunaris-dark.js",
      "require": "./dist/lunaris-dark.cjs"
