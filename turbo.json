{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": ["TEST_FLAG"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "dependsOn": ["^build"],
      "persistent": true,
      "cache": false
    },
    "build:doc": {
      "dependsOn": ["^build"]
    }
  },
  "ui": "tui"
}
